{
    "emmet.includeLanguages": {
        "rust": "html"
    },
    "emmet.extensionsPath": [
        "./.vscode/"
    ],
    "tailwindCSS.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": true
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "cmd": "~/.cargo/bin/leptosfmt '${file}'",
                "match": "\\.rs$"
            }
        ]
    },
    "css.validate": false,
    "tailwindCSS.classAttributes": [
        "class",
        "class_textarea",
        "class_label",
    ],
}